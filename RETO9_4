def bubble_sort(lista: list) -> None:
    n: int = len(lista)
    for i in range(n):
        for j in range(n - i - 1):
            if lista[j] > lista[j + 1]:
                temp = lista[j]
                lista[j] = lista[j + 1]
                lista[j + 1] = temp

# Ejemplo
datos = [5, 3, 8, 4, 2]
bubble_sort(datos)
print("Ordenado:", datos)
